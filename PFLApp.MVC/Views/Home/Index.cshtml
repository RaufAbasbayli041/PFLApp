		<main>
			<div class="container-fluid px-4">
				<h1 class="mt-4">Statistika</h1>

				<div class="card mb-4">
					<div class="card-header">
						<i class="fas fa-table me-1"></i>
						Turnir cedveli
					</div>
					<div class="card-body">
						<table id="datatablesSimple" class="table table-bordered ">

							<thead>
								<tr>
									<th scope="col">Klublar</th>
									<th scope="col">O</th>
									<th scope="col">G</th>
									<th scope="col">H</th>
									<th scope="col">M</th>
									<th scope="col">VQ</th>
									<th scope="col">BQ</th>
									<th scope="col">TF</th>
									<th scope="col">X</th>
								</tr>
							</thead>
							<tbody>
								@* <tr>
								<th scope="col">Klublar</th>
								<th scope="col">O</th>
								<th scope="col">G</th>
								<th scope="col">H</th>
								<th scope="col">M</th>
								<th scope="col">VQ</th>
								<th scope="col">BQ</th>
								<th scope="col">TF</th>
								<th scope="col">X</th>
								</tr> *@
								<tr>
									@foreach (var team in Model)
									{
										<th scope="row">@team.Id</th>
										<td>@team.Name</td>
										<td>@team.Wins</td>
										<td>@team.Draws</td>
										<td>@team.Losses</td>
										@* <td>@team.GoalsFor</td>
										<td>@team.GoalsAgainst</td>
										<td>@team.PointsFor</td>
										<td>@team.PointsAgainst</td> *@
									}
								</tr>
								<tr>
									<th scope="row">3</th>
									<td>John</td>
									<td>Doe</td>
									<td></td>
									<td></td>
									<td></td>
									<td></td>
									<td></td>
									<td></td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
